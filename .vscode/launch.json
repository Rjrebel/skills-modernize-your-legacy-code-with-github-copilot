{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Accounting App",
      "program": "${workspaceFolder}/src/accounting/index.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Tests",
      "program": "${workspaceFolder}/src/accounting/node_modules/jest/bin/jest.js",
      "args": ["--runInBand"],
      "cwd": "${workspaceFolder}/src/accounting",
      "console": "integratedTerminal"
    }
  ]
}
